<Window x:Class="RevitReactionImporter.DataFileBrowser"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RAM Data File Browswer" Height="100" Width="300">
    <DockPanel Margin="10">
        <WrapPanel HorizontalAlignment="Right" DockPanel.Dock="Top" Margin="0,0,0,10">
            <TextBox Name="txtEditor" Width="210" />
            <Button Name="btnOpenFile" Click="onBrowseFileClick">Browse...</Button>
        </WrapPanel>
    </DockPanel>
</Window>
